{
  "name": "mapitout-client",
  "version": "0.0.1",
  "description": "Mapitout client",
  "scripts": {
    "start": "serve ../docs",
    "dev": "webpack-dev-server --open --config webpack.dev.js",
    "build": "rm -rf ../docs && webpack -p --config webpack.prod.js --optimize-minimize && npm run copy-files",
    "copy-files": "npm run copy-sw && npm run copy-manifest && npm run copy-readme",
    "create-cname": "touch ../docs/CNAME && echo \"www.mapitout.com\" >> ../docs/CNAME ",
    "copy-sw": "cp src/services/service-worker.js ../docs",
    "copy-manifest": "cp src/manifest.json ../docs",
    "copy-readme": "cp ./readme.md ../docs"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.5.4",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "image-webpack-loader": "^4.4.0",
    "json-loader": "^0.5.7",
    "no-console-webpack-plugin": "0.0.5",
    "node-sass": "^4.13.1",
    "sass-loader": "^6.0.6",
    "serve": "^6.1.0",
    "style-loader": "^0.20.1",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-dev-server": "^2.11.5",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap": "^4.0.0-beta.2",
    "lodash": "^4.17.15",
    "mapbox-gl": "^1.8.1",
    "prop-types": "^15.6.0",
    "qs": "^6.5.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-dropzone": "^4.2.8",
    "react-image": "^1.3.1",
    "react-map-gl": "^5.2.3",
    "react-map-gl-geocoder": "^2.0.11",
    "react-recaptcha": "^2.3.10",
    "react-redux": "^5.0.6",
    "react-redux-loading-bar": "^3.1.1",
    "react-router-dom": "^4.2.2",
    "recharts": "^1.8.5",
    "redux": "^3.7.2",
    "redux-form": "^7.2.3",
    "redux-promise-middleware": "^5.0.0",
    "redux-thunk": "^2.1.0",
    "superagent": "^3.8.2"
  }
}
